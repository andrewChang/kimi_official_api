{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "npm: dev",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "dev"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "restart": true,
        "timeout": 30000,
        "outputCapture": "std"
      }
    ]
  }
  